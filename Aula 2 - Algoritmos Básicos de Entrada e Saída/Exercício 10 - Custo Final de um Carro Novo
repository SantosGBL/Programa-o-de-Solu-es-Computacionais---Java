//10. O custo de um carro novo ao consumidor é a soma do custo de fábrica com a
//porcentagem do distribuidor e dos impostos (aplicados ao custo de fábrica). Supondo que o
//percentual do distribuidor seja de 28% e os impostos de 45%, ler o custo de fábrica de um
//carro, calcular e escrever o custo final ao consumidor.

package com.mycompany.custodoautomovel;

import javax.swing.JOptionPane;

public class CustoDoAutomovel {

    public static void main(String[] args) {
       
        double CustoFinal,
                CustodeFabrica = Double.parseDouble(JOptionPane.showInputDialog
        ("Informe o custo de fábrica"));
        
        CustoFinal = CustodeFabrica + CustodeFabrica * 0.28 + CustodeFabrica * 0.45;
        
        JOptionPane.showMessageDialog(null, "O custo final do veiculo é:" + CustoFinal);
    }
}
