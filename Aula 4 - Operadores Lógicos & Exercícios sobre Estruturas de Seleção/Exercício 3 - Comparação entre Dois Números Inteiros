//3. Escreva um programa que faz a leitura de 2 números inteiros. Caso o primeiro seja maior do que o 
//segundo, o programa imprime “primeiro maior do que o segundo”. Caso contrário, o programa imprime 
//“segundo maior do que o primeiro”.
package com.mycompany.comparacaoentredoisnumerosinteiros;

import javax.swing.JOptionPane;

public class ComparacaoentreDoisNumerosInteiros {

    public static void main(String[] args) {
        int valor1 = Integer.parseInt(JOptionPane.showInputDialog("Informe o primeiro valor"));
        int valor2 = Integer.parseInt(JOptionPane.showInputDialog("Informe o segundo valor"));
        
        if (valor1 > valor2){
            JOptionPane.showMessageDialog(null, "O primeiro valor é maior do que o segundo valor");
        }else {
            JOptionPane.showMessageDialog(null,"O segundo valor é maior do que o primeiro valor");
        }
    }
}
