//1. Escreva um programa que exibe os primeiros 10 números naturais. Faça versões usando do/while.

package com.mycompany.primeiros10numerosnaturaisdowhile;

import javax.swing.JOptionPane;

public class Primeiros10NumerosNaturaisDoWhile {

    public static void main(String[] args) {
        String resultado = "";
        int i = 0;
        
        do{
           resultado += i + "\n"; 
        i++;
    } while
        (i <= 9);
        
        JOptionPane.showMessageDialog(null, "Os 10 primeiros números naturais são: \n"+ resultado);
    }
}
