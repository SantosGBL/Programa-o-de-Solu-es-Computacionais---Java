//1. Escreva um programa que exibe os primeiros 10 números naturais. Faça versões usando For

package com.mycompany.primeiros10numerosnaturaisfor;

import javax.swing.JOptionPane;

public class Primeiros10NumerosNaturaisFor {

    public static void main(String[] args) {
        String resultado = "";
        
        for(int i = 0; i <= 9; i++){
            resultado += i + "\n";
        }
        JOptionPane.showMessageDialog(null, "Os 10 primeiros números naturais são: \n"+ resultado);      
    }
}
